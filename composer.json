{
    "name": "manticoresoftware/manticoresearch-php",
    "description": "PHP Client for Manticore Search",
    "keywords": ["search","client","manticoresearch"],
    "license": "Apache-2.0",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "guzzlehttp/psr7": "1.4.2",
        "http-interop/http-factory-guzzle": "^1.0",
        "php-http/curl-client": "^2.2",
        "php-http/discovery": "^1.14",
        "php-http/message": "^1.11",
        "psr/log": ">=1.1"
    },
    "suggest": {
        "ext-curl": "*",
        "guzzlehttp/psr7": ">=1.6",
        "php-http/curl-client": ">=1.7",
        "php-http/httplug": "^1.1",
        "php-http/message": "^1.7",
        "monolog/monolog":"*"
    },
    "require-dev": {
        "mockery/mockery": "*",
        "phpunit/phpunit": ">=7.5",
        "squizlabs/php_codesniffer": ">=3.5"
    },
    "autoload": {
      "psr-4": {
          "Manticoresearch\\" : "src/Manticoresearch/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\ManticoreSearch\\": "test/Manticoresearch"
        }
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Adrian Nuta",
            "email": "adriannuta@gmail.com"
        }
    ]
}
